# Garmin Connect Developer Credentials
# Get these from: https://developerportal.garmin.com/user/me/apps?program=829

# OAuth Consumer Key from Garmin Developer Portal
VITE_GARMIN_CONSUMER_KEY=your_consumer_key_here

# OAuth Consumer Secret from Garmin Developer Portal (keep this secure)
GARMIN_CONSUMER_SECRET=your_consumer_secret_here

# OAuth Redirect URI (must match registered URI in Garmin Developer Portal)
VITE_GARMIN_REDIRECT_URI=http://localhost:5173/garmin/callback

# Production Redirect URI for stridr.dev
VITE_GARMIN_REDIRECT_URI_PROD=https://stridr.dev/garmin/callback

# OAuth Scopes (Training API requires WORKOUT_IMPORT permission)
VITE_GARMIN_SCOPES=WORKOUT_IMPORT

# Set to true for production-level API credentials
VITE_GARMIN_PRODUCTION_MODE=false

# Base URLs
VITE_GARMIN_API_BASE_URL=https://apis.garmin.com
VITE_GARMIN_CONNECT_BASE_URL=https://connect.garmin.com

